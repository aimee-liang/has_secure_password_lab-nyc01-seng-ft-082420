<%= form_for @user, url: login_path, method: :post do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit "Log in" %>
    Or <%= link_to(controller: 'sessions', action 'new') {'sign up'} %>
<% end %>